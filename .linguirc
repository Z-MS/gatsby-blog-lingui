{
  "locales": ["en", "de"],
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "<rootDir>/i18n/lingui/{locale}/messages",
      "include": ["<rootDir>/src"],
      "exclude": ["**/node_modules/**", "**/__tests__/**"]
    }
  ],
  "format": "minimal",
  "extractBabelOptions": {
    "presets": ["babel-preset-gatsby"]
  }
}